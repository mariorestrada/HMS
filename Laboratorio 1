# Paso 1: Imprimir "Hello World"
print("Hello World")

# Paso 2: Importar módulos
import numpy as np
import matplotlib.pyplot as plt
import os

# Paso 3: Definir una lista
mi_lista = [1, 2, 3, 4, 5]

# Paso 4: Definir ciclo for para imprimir la lista
print("Imprimiendo la lista:")
for elemento in mi_lista:
    print(elemento)

